<?xml version="1.0"?>
<PropertyList>
<generic>
   <output>
      <line_separator>newline</line_separator>
      <var_separator>;</var_separator>

      <chunk>
         <name>IAS</name>
         <node>/autopilot/settings/target-speed-kt</node>
         <type>int</type>
         <format>%d</format>
       </chunk>

      <chunk>
         <name>HDG</name>
         <node>/autopilot/settings/heading-bug-deg</node>
         <type>int</type>
         <format>%d</format>
       </chunk>

      <chunk>
         <name>ALT</name>
         <node>/autopilot/settings/counter-set-altitude-ft</node>
         <type>int</type>
         <format>%d</format>
       </chunk>

   </output>
</generic>
</PropertyList>
